{
    "manifest_version": 3,
    "name": "OpenStore Wallet Sample",
    "version": "1.0.0",
    "description": "Sample Wallet Extension",
    "permissions": [
        "storage",
        "sidePanel"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {},
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; connect-src *; img-src * data: https:;"
    }
}